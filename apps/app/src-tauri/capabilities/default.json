{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "dialog:default",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-create",
    "fs:allow-write",
    "fs:allow-app-write"
  ]
}
